{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "npmScope": "sagi",
  "affected": {
    "defaultBase": "main"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"],
        "accessToken": "OWY1OTYwNzAtYTI3Zi00MDE3LTgyMWEtMDkyZTE1MzYxYjVlfHJlYWQtd3JpdGU="
      }
    }
  },
  "targetDependencies": {
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ]
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "none",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "none",
        "pascalCaseFiles": true
      },
      "library": {
        "style": "none",
        "linter": "eslint"
      }
    },
    "@nrwl/react-native": {
      "component": {
        "style": "none",
        "pascalCaseFiles": true
      }
    }
  },
  "defaultProject": "mobile"
}
